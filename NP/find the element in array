#include<iostream>
using namespace std ;

 int check( int number ,   int array[] ,int size  ){
int count =0;
for (int i = 0; i < size; i++)
{
    if (array[i]== number  )
    {
    count++ ; 
    }
    
}

return count ;
}
int main (){

int count ;
int array []={1,6,9,45,6} ;
 int size = sizeof(array) / sizeof(array[0]);
 int  number ;
 cout<<"enter the value of number ";
cin>>number;
 int result=check(number, array,size);

cout<<count ;


    return 0 ;
}
