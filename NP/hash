// this code require input at the same time and not after running the problem 
// run prefectly on codechef,
#include<iostream>
#include<bits/stdc++.h>

using  namespace std ;
int main (){

int n ;
cin>>n;
int arr[n];
for (int  i = 0; i < n; i++)
{
   cin>>arr[i];
}
//precompute
int hash[]={0};
for (int i = 0; i < n; i++)
{
    hash[arr[i]]+=1;
}
int q;
cin>>q;
while (q--)
{
     int number;
     cin>>number;
     //fetch
     cout<<hash[number];
}




    return 0 ; 
}
